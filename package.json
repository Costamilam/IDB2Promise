{
    "name": "idb2promise",
    "version": "0.0.1",
    "description": "TypeScript library to manage IndexedDB Storage",
    "main": "src/main.ts",
    "scripts": {
        "build": "npm run build:no-min && npm run build:min",
        "build:no-min": "node_modules/.bin/rollup -c rollup.config.js --environment BUILD:development",
        "build:min": "node_modules/.bin/rollup -c rollup.config.js --environment INCLUDE_DEPS,BUILD:minify",
        "test": "export CHROME_BIN=chromium-browser && karma start"
    },
    "keywords": [
        "javascript",
        "typescript",
        "library",
        "indexeddb"
    ],
    "author": "Guilherme Costamilam",
    "license": "ISC",
    "dependencies": {},
    "devDependencies": {
        "@types/es6-promise": "^3.3.0",
        "@types/jasmine": "^3.4.6",
        "karma": "^4.4.1",
        "karma-chrome-launcher": "^3.1.0",
        "karma-jasmine": "^2.0.1",
        "karma-typescript": "^4.1.1",
        "rollup": "^1.27.0",
        "rollup-plugin-babel-minify": "^9.1.1",
        "rollup-plugin-buble": "^0.19.8",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-typescript": "^1.0.1",
        "rollup-plugin-uglify": "^6.0.3",
        "tslib": "^1.10.0",
        "typescript": "^3.7.2"
    }
}
